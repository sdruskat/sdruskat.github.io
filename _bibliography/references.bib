@unpublished{druskat-2017-rse-opentechsummit,
  author = {Stephan Druskat},
  title = {{Research Software Engineers}: The people behind research software},
  note = {OpenTechSummit 2017},
  month = may,
  year = {2017},
  abstract = {Software has become a cornerstone of scientific research. At least since the advent of the Digital Humanities it has undoubtedly influenced methods of, and provided research objects for, almost every scientific discipline. Software development, therefore, has in turn itself become an essential part of research. The importance of research software and its development for the research process, however, is still not sufficiently mapped to recognition, reward systems, reputation, visibility, availability, job decriptions, trainings, quality standards and academic structures. Research Software Engineers (RSEs) are software developers, coding researchers, innovators, scripting administrators, software project managers, and everyone else, regardless of skill sets, who is involved in software-related work; simply put: "people who work in research and care about software". They are all brought together in the RSE community to set a focus on current challenges and develop solutions. In Germany, they are represented by the de-RSE chapter, a young and continuously growing federal initiative. It provides a forum for all those with an interest in the development of research software and its role in the academic ecosystem, irrespective of their discipline, job description, education and experience.},
  howpublished = {Talk given at OpenTechSummit 2017, Potsdam, Germany, 25 May 2017}
}

